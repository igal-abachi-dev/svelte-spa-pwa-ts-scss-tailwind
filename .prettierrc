{
  "bracketSpacing": true,
  
  "singleQuote": true,
  "trailingComma": "all",
  "useTabs": true,
  "semi": true,
  "tabWidth": 4,
  "printWidth": 100,
  "plugins": ['prettier-plugin-svelte'],
  "overrides": [
    {
      "files": "*.ts",
      "options": {
        "parser": "typescript"
      }
    }
  ],
  "svelteSortOrder": 'styles-scripts-markup',
  "svelteStrictMode": false,
  "svelteBracketNewLine": true,
}